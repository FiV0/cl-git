#!/usr/bin/sbcl --script
; -*- mode: Lisp;-*-

(load "~/.sbclrc")
(sb-ext:enable-debugger)

(asdf:oos 'asdf:load-op :FiveAM)
(asdf:oos 'asdf:load-op :cl-git)
(asdf:oos 'asdf:load-op :cl-git-tests)

(use-package 'cl-git-tests)

(FiveAM:run! (list 'repository-init
                   'create-commit
                   'create-random-commits))
